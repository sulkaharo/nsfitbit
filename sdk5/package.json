{
  "name": "NightscoutMonitor",
  "version": "1.6.5",
  "private": false,
  "license": "LGPL-2.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/sulkaharo/nsfitbit"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@fitbit/sdk": "^6.1.0",
    "@fitbit/sdk-cli": "^1.7.3",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "jshint": "^2.10.3",
    "mocha": "^6.2.2",
    "should": "^13.2.3"
  },
  "fitbit": {
    "appUUID": "eaed806d-9ff5-4aa9-a2c8-518f1f852f5a",
    "appType": "clockface",
    "appDisplayName": "NightscoutMonitor",
    "iconFile": "resources/icon.png",
    "wipeColor": "#3f51b5",
    "requestedPermissions": [
      "access_internet",
      "run_background",
      "access_activity",
      "access_heart_rate",
      "access_user_profile"
    ],
    "buildTargets": [
      "atlas",
      "vulcan"
    ],
    "i18n": {
      "en": {
        "name": "NightscoutMonitor"
      }
    },
    "defaultLanguage": "en-US"
  },
  "scripts": {
    "build": "fitbit-build",
    "prebuild": "mkdir app; cp -r ../app/* app/; mkdir companion; cp -r ../companion/* companion/; mkdir settings; cp -r ../settings/* settings/; mkdir resources; cp -r ../resources/* resources/;  cp -r resources_sdk5/* resources/",
    "test": "yarn run prebuild; mkdir tests; cp -r ../tests/* tests/; mocha --require @babel/register --exit tests/*.test.js",
    "debug": "fitbit"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  }
}
